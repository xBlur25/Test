CreateTeleport("RPG", 115, -28, -275)
CreateTeleport("armors", -1180, 29, -488)
CreateTeleport("Flame", -153.5, 55, -100)
CreateTeleport("police", -267, 20, -95)
CreateTeleport("fitness", -75, 20, -635)
CreateTeleport("Grenade", -960, 0, 470)
